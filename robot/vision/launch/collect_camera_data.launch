<launch>
    <include file="$(find realsense2_camera)/launch/rs_rgbd.launch">
        <arg name="fisheye_width"   value="1920"/>
        <arg name="fisheye_height"  value="1080"/>

        <arg name="depth_width"     value="1280"/>
        <arg name="depth_height"    value="720"/>

        <arg name="color_width"     value="1920"/>
        <arg name="color_height"    value="1080"/>

        <arg name="infra1_width"    value="1280"/>
        <arg name="infra1_height"   value="720"/>
        <arg name="infra2_width"    value="1280"/>
        <arg name="infra2_height"   value="720"/>
    </include>
</launch>